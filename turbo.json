{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "build/**/js/**/*",
        "build/**/css/**/*"
      ]
    },
    "type-check": {
      "dependsOn": [
        "^type-check"
      ],
      "outputs": [
        "build/ts/**/*"
      ]
    },
    "clean": {},
    "test": {
      "dependsOn": [
        "^test"
      ]
    },
    "pack": {
      "dependsOn": [
        "^build"
      ]
    }
  }
}