{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "build/**/js/**/*",
        "build/**/css/**/*"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ],
      "outputs": [
        "build/ts/**/*"
      ]
    },
    "clean": {}
  }
}